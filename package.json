{
  "name": "swagger2api",
  "displayName": "swagger2api",
  "description": "Quickly converting api document to usable Front-End code.",
  "version": "0.1.1",
  "publisher": "liik",
  "repository": "https://github.com/Liiked/swagger2api",
  "icon": "media/icon.png",
  "keywords": [
    "swagger",
    "api"
  ],
  "categories": [
    "Formatters"
  ],
  "engines": {
    "vscode": "^1.40.0"
  },
  "activationEvents": [
    "workspaceContains:package.json",
    "onCommand:s2a.test.viewManage.configSelector"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger-to-api",
          "title": "swagger2api",
          "icon": "media/transfer.svg"
        }
      ]
    },
    "views": {
      "swagger-to-api": [
        {
          "id": "swaggerToApi",
          "name": "Swagger To API"
        }
      ]
    },
    "commands": [
      {
        "command": "s2a.test.configProvider.genMetaData",
        "title": "测试:配置管理-生成元数据",
        "category": "swagger2api"
      },
      {
        "command": "s2a.test.viewManage.errorManage",
        "title": "测试:错误处理",
        "category": "swagger2api"
      },
      {
        "command": "s2a.test.viewManage.configSelector",
        "title": "测试:视图管理-配置选择器",
        "category": "swagger2api"
      },
      {
        "command": "s2a.test.storeManage.genConfig",
        "title": "测试:储存器-生成config",
        "category": "swagger2api"
      },
      {
        "command": "s2a.test.exportSingleAPI",
        "title": "测试:视图管理-导出单个api",
        "category": "swagger2api"
      },
      {
        "command": "s2a.fetchSourceBySelect",
        "title": "Select Source Data",
        "category": "swagger2api"
      },
      {
        "command": "s2a.fetchSourceData",
        "title": "Fetch Source Data",
        "category": "swagger2api"
      },
      {
        "command": "s2a.test.refresh",
        "title": "Refresh",
        "category": "swagger2api",
        "icon": {
          "light": "media/light/refresh.svg",
          "dark": "media/dark/refresh.svg"
        }
      },
      {
        "command": "s2a.test.trace",
        "title": "Trace Source",
        "category": "swagger2api",
        "icon": {
          "light": "media/light/download.svg",
          "dark": "media/dark/download.svg"
        }
      },
      {
        "command": "s2a.test.confirmTraceResult",
        "title": "Confirm This Change",
        "category": "swagger2api",
        "icon": {
          "light": "media/light/checkmark.svg",
          "dark": "media/dark/checkmark.svg"
        }
      },
      {
        "command": "s2a.test.export",
        "title": "Export Code",
        "category": "swagger2api",
        "icon": {
          "light": "media/light/export.svg",
          "dark": "media/dark/export.svg"
        }
      },
      {
        "command": "s2a.test.exportFilter",
        "title": "Export Code By Config Filter",
        "category": "swagger2api",
        "icon": {
          "light": "media/light/exportFilter.svg",
          "dark": "media/dark/exportFilter.svg"
        }
      },
      {
        "command": "s2a.test.exportSingleAPI",
        "title": "Export Single API Code By Config Filter",
        "when": "view == swaggerToApi",
        "group": "inline"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "s2a.test.refresh",
          "when": "view == swaggerToApi",
          "group": "navigation"
        },
        {
          "command": "s2a.test.trace",
          "when": "view == swaggerToApi",
          "group": "navigation"
        },
        {
          "command": "s2a.test.confirmTraceResult",
          "when": "view == swaggerToApi",
          "group": "navigation"
        },
        {
          "command": "s2a.test.export",
          "when": "view == swaggerToApi",
          "group": "navigation"
        },
        {
          "command": "s2a.test.exportFilter",
          "when": "view == swaggerToApi",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "s2a.test.exportSingleAPI",
          "when": "view == swaggerToApi",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"ts-loader": "^8.0.3",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
  "dependencies": {
    "@types/jsonfile": "^5.0.0",
    "@types/lodash": "^4.14.149",
    "@types/prettier": "^1.19.0",
    "axios": "^0.19.0",
    "commitizen": "^4.0.3",
    "cosmiconfig": "^6.0.0",
    "fs-extra": "^8.1.0",
    "fs-tree-utils": "^0.1.8",
    "jsonfile": "^5.0.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "swagger-parser": "^8.0.2"
  }
}
