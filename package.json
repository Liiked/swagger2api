{
  "name": "swagger2api",
  "displayName": "swagger2api",
  "description": "Quickly converting api document to usable Front-End code.",
  "version": "0.1.1",
  "publisher": "liik",
  "repository": "https://github.com/Liiked/swagger2api",
  "icon": "media/icon.png",
  "keywords": [
    "swagger",
    "api"
  ],
  "categories": [
    "Formatters"
  ],
  "engines": {
    "vscode": "^1.36.0"
  },
  "activationEvents": [
    "onView:nodeDependencies",
    "onLanguage:json",
    "workspaceContains:s2a.config.js"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger-to-api",
          "title": "swagger2api",
          "icon": "media/transfer.svg"
        }
      ]
    },
    "views": {
      "swagger-to-api": [
        {
          "id": "swaggerToApi",
          "name": "Swagger To API"
        }
      ]
    },
    "commands": [
      {
        "command": "s2a.fetchSourceBySelect",
        "title": "Select Source Data",
        "category": "swagger2api"
      },
      {
        "command": "s2a.fetchSourceData",
        "title": "Fetch Source Data",
        "category": "swagger2api"
      },
      {
        "command": "s2a.readjson",
        "title": "swagger json to template",
        "category": "swagger2api",
        "icon": {
          "light": "./media/transfer.svg",
          "dark": "./media/transfer.svg"
        }
      },
      {
        "command": "s2a.quickInput",
        "title": "swagger quick input",
        "category": "swagger2api"
      },
      {
        "command": "s2a.convert",
        "title": "swagger template to api",
        "category": "swagger2api",
        "icon": {
          "light": "./media/transfer.svg",
          "dark": "./media/transfer.svg"
        }
      },
      {
        "command": "s2a.refresh",
        "title": "Refresh",
        "category": "swagger2api",
        "icon": {
          "light": "media/light/refresh.svg",
          "dark": "media/dark/refresh.svg"
        }
      },
      {
        "command": "s2a.createConfig",
        "title": "create config",
        "category": "swagger2api"
      },
      {
        "command": "s2a.clearCache",
        "title": "clear cache",
        "category": "swagger2api"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "s2a.refresh",
          "when": "view == swaggerToApi",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "s2a.readjson",
          "group": "navigation",
          "when": "resourceLangId == json"
        },
        {
          "command": "s2a.convert",
          "group": "navigation",
          "when": "resourceFilename =~ /Untitled/"
        }
      ],
      "commandPalette": [
        {
          "command": "s2a.readjson",
          "when": "resourceLangId == json"
        },
        {
          "command": "s2a.convert",
          "when": "resourceFilename =~ /Untitled/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js",
    "commit": "git cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@types/jsonfile": "^5.0.0",
    "@types/lodash": "^4.14.149",
    "axios": "^0.19.0",
    "commitizen": "^4.0.3",
    "jsonfile": "^5.0.0",
    "lodash": "^4.17.15",
    "swagger-parser": "^8.0.2"
  }
}
